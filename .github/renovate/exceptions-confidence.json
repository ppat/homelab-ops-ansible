{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchConfidence": [
        "high",
        "very high"
      ],
      "matchDatasources": [
        "pypi"
      ],
      "matchPackageNames": [
        "ansible-lint",
        "molecule",
        "molecule-plugins"
      ]
    },
    {
      "matchConfidence": [
        "low",
        "neutral"
      ],
      "matchDatasources": [
        "pypi"
      ],
      "matchPackageNames": [
        "ansible-lint",
        "molecule",
        "molecule-plugins"
      ],
      "minimumReleaseAge": "180 days"
    }
  ]
}
